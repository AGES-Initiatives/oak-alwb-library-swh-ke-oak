{'id_' : 'prayers_swh_KE_oak|AgiosOTheos|text', 'text' : 'Mungu Mtakatifu, Mweza Mtakatifu, Usiyekufa Mtakatifu, utuhurumie.'}
{'id_' : 'prayers_swh_KE_oak|EpouranieVasilef|text', 'text' : ''}
{'id_' : 'prayers_swh_KE_oak|KyrieKaiDespota1|text', 'text' : ''}
{'id_' : 'prayers_swh_KE_oak|KyrieKaiDespota2|text', 'text' : ''}
{'id_' : 'prayers_swh_KE_oak|KyrieKaiDespota3|text', 'text' : ''}
{'id_' : 'prayers_swh_KE_oak|KyrieKaiDespota4|text', 'text' : ''}
